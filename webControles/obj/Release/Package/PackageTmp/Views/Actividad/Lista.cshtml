@using Microsoft.Ajax.Utilities
@using webControles.Helpers
@model webControles.Models.ActividadModel
           

    <center>
        @Html.Partial("Paginacion/Pagina", Model.objPaginacionModel)
    </center>

    <table class="table table-striped">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Orden)
        </th>

        <th>
            @Html.DisplayNameFor(model => model.Activo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Actividad)
        </th>

        <th>
            @Html.DisplayName("Tiempo Promedio")
        </th>

        <th>
            @Html.DisplayName("")
        </th>
    </tr>

    @foreach (var item in Model.listActividadModel)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Orden)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Activo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Actividad)
            </td>

            <td>
                @Html.DisplayFor(modelItem => item.TiempoPromedioObjetivo)
            </td>
            <td>
                <div class="form-group">
                    <div class="btn-group">
                        @Html.HiddenFor(modelItem => item.IdEncActividad, new { @Id = "hiddenIdActividad" })
                        <button type="button" class="btn btn-warning" id="btnEditar" title="Editar"> <span class="glyphicon glyphicon-pencil"></span> </button>
                    </div>

                </div>
            </td>
        </tr>
    }

</table>

    <center>
        @Html.Partial("Paginacion/Pagina", Model.objPaginacionModel)
    </center>



