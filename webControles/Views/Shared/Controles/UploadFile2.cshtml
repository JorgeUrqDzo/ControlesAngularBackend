<script id="panelUploadLargeFiles" type="text/template">
    <div class='col-xs-12 col-sm-8' id='divSubirDocumentos'>
        <div class='panel panel-default'>

            <div class='panel-heading'>
                <label id='lblDocumentosCargar'>Subir Documentos</label>
            </div>
            <div class='panel-body'>
                <div class='text-center'>
                    <label id='lblDocumentosDescripcion'>Seleccione un archivo para adjuntar. Formatos permitidos .jpg, .jpeg, .gif, .png, .bmp, .pdf, .txt, .xml, .xps, .docx, .doc.</label>
                </div>
                <div id='@@Id'>
                    <br />
                    <input type="file" id="uploadFile" name="file" class="form-control" />
                    <br />
                    <input type="button" id="btnUpload" class="btn btn-primary" value="Guardar" />
                    <br />
                    <br />
                </div>
                <input type='button' id='btnGuardar' name='btnGuardar' class='btn btn-default' value='Guardar Documento' />
            </div>
        </div>
    </div>
</script>




